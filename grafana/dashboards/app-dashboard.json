          "editorMode": "code",
          "expr": "histogram_quantile(0.90, sum by (le, endpoint) (\n  rate(http_request_duration_milliseconds_bucket{instance=\"${instance}\"}[1m])\n))",
          "legendFormat": "p90 - {{endpoint}}",
          "range": true,
          "refId": "P90"
        },
        {
          "editorMode": "code",
          "expr": "histogram_quantile(0.95, sum by (le, endpoint) (rate(http_request_duration_ms_bucket{instance=\"${instance}\"}[1m])))",
          "legendFormat": "p95 - {{endpoint}}",
          "range": true,
          "refId": "P95"
        },
        {
          "editorMode": "code",
          "expr": "histogram_quantile(0.99, sum by (le, endpoint) (rate(http_request_duration_ms_bucket{instance=\"${instance}\"}[1m])))",
          "legendFormat": "p99 - {{endpoint}}",
          "range": true,
          "refId": "P99"
        },
        {
          "editorMode": "code",
          "expr": "sum by (endpoint) (rate(http_request_duration_ms_sum{instance=\"${instance}\"}[1m])) / sum by (endpoint) (rate(http_request_duration_ms_count{instance=\"${instance}\"}[1m]))",
          "legendFormat": "avg - {{endpoint}}",
          "range": true,
          "refId": "AVG"
        },
        {
          "editorMode": "code",
          "expr": "min_over_time((sum by (endpoint) (rate(http_request_duration_ms_sum{instance=\"${instance}\"}[1m])) / sum by (endpoint) (rate(http_request_duration_ms_count{instance=\"${instance}\"}[1m])))[1m:1m])",
          "legendFormat": "min - {{endpoint}}",
          "range": true,
          "refId": "MIN"
        },
        {
          "editorMode": "code",
          "expr": "max_over_time((sum by (endpoint) (rate(http_request_duration_ms_sum{instance=\"${instance}\"}[1m])) / sum by (endpoint) (rate(http_request_duration_ms_count{instance=\"${instance}\"}[1m])))[1m:1m])",
          "legendFormat": "max - {{endpoint}}",
          "range": true,
          "refId": "MAX"
        }
      ],
